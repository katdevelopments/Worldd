local function safeSetCursor(object, cursor)
pcall(function()
object.CursorIcon = cursor
end)
end
local function debounce(func, delay)
local timer
return function(...)
local context = self
local args = {...}
if timer then
task.cancel(timer)
end
timer = task.delay(delay, function()
func(context, unpack(args))
end)
end
end
