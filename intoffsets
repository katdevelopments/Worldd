// DLL OFFSET

// LUA STATE
inline uintptr_t GetGlobalState(uintptr_t ScriptContext, uintptr_t& a2, uintptr_t& a3) {
    a2 = 0;
    a3 = 0;
    inline auto GetState = reinterpret_cast<uintptr_t(__fastcall*)(uintptr_t, uint64_t*, uint64_t*)>(Offsets::GetGlobalState);
    uintptr_t globalstate = GetState(ScriptContext, &a2, &a3);
    return globalstate;
}


inline void* DecryptLuaState(uintptr_t ScriptContext)
{
    auto address = ScriptContext + 0x380;
    auto data = reinterpret_cast<uint32_t*>(address);

    uint64_t a = static_cast<uint64_t>(address);

    uint32_t low  = data[0] ^ static_cast<uint32_t>(a);
    uint32_t high = data[1] ^ static_cast<uint32_t>(a >> 32);

    void* decoded = reinterpret_cast<void*>((static_cast<uint64_t>(high) << 32) | low);

    *reinterpret_cast<bool*>(ScriptContext + 0x878) = true;

    return decoded;
}


inline void SetIdentity(lua_State* L, int64_t lvl) {
    if (!L) return;
    auto state = (__int64)L;
    if (!state) return;
    const auto ExtraSpace = *reinterpret_cast<uintptr_t*>(state + 0x68);
    if (!ExtraSpace) return;
    int64_t Caps = identity_to_caps(8);
    *reinterpret_cast<int64_t*>(ExtraSpace + 0x78) = lvl;
}

